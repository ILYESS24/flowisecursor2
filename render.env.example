# Render Environment Configuration

# Production settings
NODE_ENV=production
PORT=10000

# Database (will be set by Render)
# DATABASE_URL will be automatically provided by Render PostgreSQL

# Flowise Configuration
FLOWISE_USERNAME=admin
FLOWISE_PASSWORD=change_this_password
FLOWISE_SECRETKEY=your_secret_key_here

# API Configuration
APIKEY_PATH=/opt/render/project/src/packages/server/.flowise

# Security
CORS_ORIGINS=*
ALLOWED_IFRAME_ORIGINS=*

# Optional: Disable telemetry for production
DISABLE_TELEMETRY=true

# Optional: Enable logging
LOG_LEVEL=info
